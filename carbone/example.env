# Domain-Konfiguration für Carbone
DOMAIN_NAME=orgops.io
SUBDOMAIN=carbone

# Carbone Lizenz (leer lassen für Community Edition)
CARBONE_EE_LICENSE=

# Studio Web-Interface aktivieren
CARBONE_EE_STUDIO=true

# Authentifizierung
CARBONE_EE_AUTHENTICATION=false
CARBONE_EE_STUDIOUSER=admin:changeme

# Performance-Einstellungen
CARBONE_EE_FACTORIES=2
CARBONE_EE_ATTEMPTS=2
CARBONE_EE_CONVERTERFACTORYTIMEOUT=60000

# Limits
CARBONE_EE_MAXDATASIZE=62914560
CARBONE_EE_MAXTEMPLATESIZE=20971520

# Lokalisierung
CARBONE_EE_TIMEZONE=Europe/Berlin
CARBONE_EE_LANG=de
