# Application
APP_ID=
APP_NAME=Coolify
APP_KEY=
APP_ENV=production
APP_URL=https://deploy.orgops.io
APP_PORT=8000

# Database
DB_DATABASE=coolify
DB_USERNAME=coolify
DB_PASSWORD=

# Redis
REDIS_PASSWORD=

# Pusher/Soketi
PUSHER_APP_ID=
PUSHER_APP_KEY=
PUSHER_APP_SECRET=

# Initial Root User (optional)
ROOT_USERNAME=
ROOT_USER_EMAIL=
ROOT_USER_PASSWORD=

# Registry
REGISTRY_URL=ghcr.io
LATEST_IMAGE=latest

# PHP Settings
PHP_MEMORY_LIMIT=256M
PHP_FPM_PM_CONTROL=dynamic
PHP_FPM_PM_START_SERVERS=1
PHP_FPM_PM_MIN_SPARE_SERVERS=1
PHP_FPM_PM_MAX_SPARE_SERVERS=10

# Debug
SOKETI_DEBUG=false

# Auto-update
AUTOUPDATE=false

# Instance ID
COOLIFY_INSTANCE_ID=

# PROXY DEAKTIVIERUNG - WICHTIG!
COOLIFY_DEFAULT_PROXY=disabled
COOLIFY_PROXY_ENABLED=false
DISABLE_PROXY=true
NO_PROXY_MANAGEMENT=true

# Setze explizit auf keinen Proxy
PROXY_TYPE=none
PROXY_ENABLED=false